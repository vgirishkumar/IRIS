<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="1.0" xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx" xmlns:d="http://schemas.microsoft.com/ado/2007/08/dataservices" xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata">
    <edmx:DataServices m:DataServiceVersion="1.0">
        <Schema xmlns="http://schemas.microsoft.com/ado/2006/04/edm" Namespace="${artifactId}Model">
            <EntityType Name="Airport">
                <Key>
                    <PropertyRef Name="code"/>
                </Key>
                <Property Name="name" Type="Edm.String" Nullable="true"/>
                <Property Name="code" Type="Edm.String" Nullable="false"/>
                <Property Name="country" Type="Edm.String" Nullable="true"/>
                <NavigationProperty Name="departures" Relationship="${artifactId}Model.FlightSchedule_departureAirport" FromRole="Airport" ToRole="FlightSchedule"/>
                <NavigationProperty Name="arrivals" Relationship="${artifactId}Model.FlightSchedule_arrivalAirport" FromRole="Airport" ToRole="FlightSchedule"/>
            </EntityType>
            <EntityType Name="Flight">
                <Key>
                    <PropertyRef Name="flightID"/>
                </Key>
                <Property Name="flightID" Type="Edm.Int64" Nullable="false"/>
                <Property Name="flightScheduleNum" Type="Edm.Int64" Nullable="false"/>
                <Property Name="takeoffTime" Type="Edm.DateTime" Nullable="true"/>
                <NavigationProperty Name="flightSchedule" Relationship="${artifactId}Model.Flight_flightSchedule" FromRole="Flight" ToRole="FlightSchedule"/>
            </EntityType>
            <EntityType Name="FlightSchedule">
                <Key>
                    <PropertyRef Name="flightScheduleID"/>
                </Key>
                <Property Name="flightNo" Type="Edm.String" Nullable="true"/>
                <Property Name="arrivalAirportCode" Type="Edm.String" Nullable="true"/>
                <Property Name="arrivalTime" Type="Edm.Time" Nullable="true"/>
                <Property Name="departureTime" Type="Edm.Time" Nullable="true"/>
                <Property Name="departureAirportCode" Type="Edm.String" Nullable="true"/>
                <Property Name="flightScheduleID" Type="Edm.Int64" Nullable="false"/>
                <Property Name="firstDeparture" Type="Edm.DateTime" Nullable="true"/>
                <Property Name="lastDeparture" Type="Edm.DateTime" Nullable="true"/>
                <NavigationProperty Name="departureAirport" Relationship="${artifactId}Model.FlightSchedule_departureAirport" FromRole="FlightSchedule" ToRole="Airport"/>
                <NavigationProperty Name="arrivalAirport" Relationship="${artifactId}Model.FlightSchedule_arrivalAirport" FromRole="FlightSchedule" ToRole="Airport"/>
                <NavigationProperty Name="flights" Relationship="${artifactId}Model.Flight_flightSchedule" FromRole="FlightSchedule" ToRole="Flight"/>
            </EntityType>
            <EntityType Name="Passenger">
                <Key>
                    <PropertyRef Name="passengerNo" />
                </Key>
                <Property Name="dateOfBirth" Type="Edm.DateTime" Nullable="false" />
                <Property Name="passengerNo" Type="Edm.Int64" Nullable="false" />
                <Property Name="flightID" Type="Edm.Int64" Nullable="false" />
                <Property Name="name" Type="Edm.String" Nullable="true" />
                <NavigationProperty Name="flight" Relationship="${artifactId}Model.Passenger_flight" FromRole="Passenger" ToRole="Flight"/>
            </EntityType>
            <Association Name="Flight_flightSchedule">
                <End Role="Flight" Type="${artifactId}Model.Flight" Multiplicity="*"/>
                <End Role="FlightSchedule" Type="${artifactId}Model.FlightSchedule" Multiplicity="0..1"/>
                <ReferentialConstraint>
                    <Principal Role="FlightSchedule">
                        <PropertyRef Name="flightScheduleID"/>
                    </Principal>
                    <Dependent Role="Flight">
                        <PropertyRef Name="flightScheduleNum"/>
                    </Dependent>
                </ReferentialConstraint>
            </Association>
            <Association Name="FlightSchedule_departureAirport">
                <End Role="FlightSchedule" Type="${artifactId}Model.FlightSchedule" Multiplicity="*"/>
                <End Role="Airport" Type="${artifactId}Model.Airport" Multiplicity="0..1"/>
                <ReferentialConstraint>
                	<Principal Role="Airport">
                		<PropertyRef Name="code"/>
                	</Principal>
                	<Dependent Role="FlightSchedule">
                		<PropertyRef Name="departureAirportCode"/>
                	</Dependent>
                </ReferentialConstraint>
            </Association>
            <Association Name="FlightSchedule_arrivalAirport">
                <End Role="FlightSchedule" Type="${artifactId}Model.FlightSchedule" Multiplicity="*"/>
                <End Role="Airport" Type="${artifactId}Model.Airport" Multiplicity="0..1"/>
                <ReferentialConstraint>
                	<Principal Role="Airport">
                		<PropertyRef Name="code"/>
                	</Principal>
                	<Dependent Role="FlightSchedule">
                		<PropertyRef Name="arrivalAirportCode"/>
                	</Dependent>
                </ReferentialConstraint>
            </Association>
            <Association Name="Passenger_flight">
                <End Role="Flight" Type="${artifactId}Model.Flight" Multiplicity="0..1"/>
                <End Role="Passenger" Type="${artifactId}Model.Passenger" Multiplicity="*"/>
                <ReferentialConstraint>
                	<Principal Role="Flight">
                		<PropertyRef Name="flightID"/>
                	</Principal>
                	<Dependent Role="Passenger">
                		<PropertyRef Name="flightID"/>
                	</Dependent>
                </ReferentialConstraint>
            </Association>
            <EntityContainer Name="${artifactId}" m:IsDefaultEntityContainer="true">
                <EntitySet Name="Airports" EntityType="${artifactId}Model.Airport"/>
                <EntitySet Name="Flights" EntityType="${artifactId}Model.Flight"/>
                <EntitySet Name="FlightSchedules" EntityType="${artifactId}Model.FlightSchedule"/>
                <EntitySet Name="Passengers" EntityType="${artifactId}Model.Passenger"/>
                <AssociationSet Name="Flight_flightSchedule" Association="${artifactId}Model.Flight_flightSchedule">
                    <End Role="Flight" EntitySet="Flights"/>
                    <End Role="FlightSchedule" EntitySet="FlightSchedules"/>
                </AssociationSet>
                <AssociationSet Name="FlightSchedule_departureAirport" Association="${artifactId}Model.FlightSchedule_departureAirport">
                    <End Role="FlightSchedule" EntitySet="FlightSchedules"/>
                    <End Role="Airport" EntitySet="Airports"/>
                </AssociationSet>
                <AssociationSet Name="FlightSchedule_arrivalAirport" Association="${artifactId}Model.FlightSchedule_arrivalAirport">
                    <End Role="FlightSchedule" EntitySet="FlightSchedules"/>
                    <End Role="Airport" EntitySet="Airports"/>
                </AssociationSet>
                <AssociationSet Name="Passenger_flight" Association="${artifactId}Model.Passenger_flight">
                    <End Role="Flight" EntitySet="Flights"/>
                    <End Role="Passenger" EntitySet="Passengers"/>
                </AssociationSet>
            </EntityContainer>
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>
