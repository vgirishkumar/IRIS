<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="1.0" xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx" xmlns:d="http://schemas.microsoft.com/ado/2007/08/dataservices" xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata">
    <edmx:DataServices m:DataServiceVersion="1.0">
        <Schema xmlns="http://schemas.microsoft.com/ado/2006/04/edm" Namespace="com.mycorp.airtraffic.FlightResponder.model">
            <EntityType Name="FlightSchedule">
                <Key>
                    <PropertyRef Name="flightScheduleID"/>
                </Key>
                <Property Name="flightNo" Type="Edm.String" Nullable="true"/>
                <Property Name="arrivalAirportCode" Type="Edm.String" Nullable="true"/>
                <Property Name="arrivalTime" Type="Edm.Time" Nullable="true"/>
                <Property Name="departureTime" Type="Edm.Time" Nullable="true"/>
                <Property Name="departureAirportCode" Type="Edm.String" Nullable="true"/>
                <Property Name="flightScheduleID" Type="Edm.Int64" Nullable="false"/>
                <Property Name="firstDeparture" Type="Edm.DateTime" Nullable="true"/>
                <Property Name="lastDeparture" Type="Edm.DateTime" Nullable="true"/>
                <NavigationProperty Name="departureAirport" Relationship="com.mycorp.airtraffic.FlightResponder.model.FlightSchedule_departureAirport" FromRole="FlightSchedule" ToRole="Airport"/>
                <NavigationProperty Name="arrivalAirport" Relationship="com.mycorp.airtraffic.FlightResponder.model.FlightSchedule_arrivalAirport" FromRole="FlightSchedule" ToRole="Airport"/>
            </EntityType>
            <EntityType Name="Airport">
                <Key>
                    <PropertyRef Name="code"/>
                </Key>
                <Property Name="name" Type="Edm.String" Nullable="true"/>
                <Property Name="code" Type="Edm.String" Nullable="false"/>
                <Property Name="country" Type="Edm.String" Nullable="true"/>
                <NavigationProperty Name="flightSchedules" Relationship="com.mycorp.airtraffic.FlightResponder.model.FlightSchedule_Airport" FromRole="Airport" ToRole="FlightSchedule"/>
            </EntityType>
            <EntityType Name="Flight">
                <Key>
                    <PropertyRef Name="flightID"/>
                </Key>
                <Property Name="flightID" Type="Edm.Int64" Nullable="false"/>
                <Property Name="takeoffTime" Type="Edm.DateTime" Nullable="true"/>
            </EntityType>
            <Association Name="FlightSchedule_Airport">
                <End Role="FlightSchedule" Type="com.mycorp.airtraffic.FlightResponder.model.FlightSchedule" Multiplicity="*"/>
                <End Role="Airport" Type="com.mycorp.airtraffic.FlightResponder.model.Airport" Multiplicity="0..1"/>
            </Association>
            <Association Name="FlightSchedule_departureAirport">
                <End Role="FlightSchedule" Type="com.mycorp.airtraffic.FlightResponder.model.FlightSchedule" Multiplicity="*"/>
                <End Role="Airport" Type="com.mycorp.airtraffic.FlightResponder.model.Airport" Multiplicity="0..1"/>
				<ReferentialConstraint>
					<Principal Role="Airport">
						<PropertyRef Name="code"/>
					</Principal>
					<Dependent Role="FlightSchedule">
						<PropertyRef Name="departureAirportCode"/>
					</Dependent>
				</ReferentialConstraint>
            </Association>
            <Association Name="FlightSchedule_arrivalAirport">
                <End Role="FlightSchedule" Type="com.mycorp.airtraffic.FlightResponder.model.FlightSchedule" Multiplicity="*"/>
                <End Role="Airport" Type="com.mycorp.airtraffic.FlightResponder.model.Airport" Multiplicity="0..1"/>
				<ReferentialConstraint>
					<Principal Role="Airport">
						<PropertyRef Name="code"/>
					</Principal>
					<Dependent Role="FlightSchedule">
						<PropertyRef Name="arrivalAirportCode"/>
					</Dependent>
				</ReferentialConstraint>
            </Association>
            <EntityContainer Name="com.mycorp.airtraffic.FlightResponder" m:IsDefaultEntityContainer="true">
                <EntitySet Name="FlightSchedule" EntityType="com.mycorp.airtraffic.FlightResponder.model.FlightSchedule"/>
                <EntitySet Name="Airport" EntityType="com.mycorp.airtraffic.FlightResponder.model.Airport"/>
                <EntitySet Name="Flight" EntityType="com.mycorp.airtraffic.FlightResponder.model.Flight"/>
                <AssociationSet Name="FlightSchedule_Airport" Association="com.mycorp.airtraffic.FlightResponder.model.FlightSchedule_Airport">
                    <End Role="FlightSchedule" EntitySet="FlightSchedule"/>
                    <End Role="Airport" EntitySet="Airport"/>
                </AssociationSet>
                <AssociationSet Name="FlightSchedule_departureAirport" Association="com.mycorp.airtraffic.FlightResponder.model.FlightSchedule_departureAirport">
                    <End Role="FlightSchedule" EntitySet="FlightSchedule"/>
                    <End Role="Airport" EntitySet="Airport"/>
                </AssociationSet>
                <AssociationSet Name="FlightSchedule_arrivalAirport" Association="com.mycorp.airtraffic.FlightResponder.model.FlightSchedule_arrivalAirport">
                    <End Role="FlightSchedule" EntitySet="FlightSchedule"/>
                    <End Role="Airport" EntitySet="Airport"/>
                </AssociationSet>
            </EntityContainer>
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>
